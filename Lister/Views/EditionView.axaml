							<Canvas
								Name="topSpan"
								Canvas.Left="0"
								Canvas.Top="15"
								Width="15"
								Height="{Binding   Path = TopSpanHeight,  Mode=OneWay}"
								Background="White"
								PointerPressed=""/>

							<Canvas
								Name="runner"
								Canvas.Left="0"
								Canvas.Top="{Binding   Path = ScrollOffset,  Mode=OneWay}"
								Width="15"
								Height="{Binding   Path = RunnerHeight,  Mode=OneWay}"
								
								Background="Red"
								PointerPressed=""
								PointerMoved=""/>

							<Canvas
								Name="bottomSpan"
								Canvas.Left="0"
								Canvas.Bottom="15"
								Width="15"
								Height="{Binding   Path = BottomSpanHeight,  Mode=OneWay}"
								Background="LightGreen"
								PointerPressed=""/>

							<Canvas
								Name="downer"
								Canvas.Left="0"
