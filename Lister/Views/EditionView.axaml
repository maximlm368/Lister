<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="460"
			 xmlns:vm="clr-namespace:Lister.ViewModels"
			 xmlns:uc="clr-namespace:Lister.Views"
			 x:Class="Lister.Views.BadgeEditorView"
			 x:DataType="vm:BadgeEditorViewModel"
			 Height="460"
			 Width="800"
			 >
	
	<StackPanel
		Name="editionPanel"
		ZIndex="1"
		Focusable="True"
		KeyDown="ToSide">

		<Grid
			Name="backBlock"
			Height="40"
			Background="LightGray"
			Margin="0,0">

			<Grid.ColumnDefinitions>
				<ColumnDefinition ></ColumnDefinition>
				<ColumnDefinition Width="240" ></ColumnDefinition>
				<ColumnDefinition ></ColumnDefinition>
			</Grid.ColumnDefinitions>

			<Grid.RowDefinitions>
				<RowDefinition Height="20" ></RowDefinition>
				<RowDefinition Height="20" ></RowDefinition>
			</Grid.RowDefinitions>

			<Button
				Name="Back"
				Classes="mainButton"
				ToolTip.Tip="Вернуться в окно построения с сохранением изменений"
				Grid.Column="1"
				Grid.Row="0"
				Grid.RowSpan="2"
				Width="200"
				Height="32"
				Margin="20,-2"
				ZIndex="2"
				Command="{Binding  Path = GoBackCommand}">

				<StackPanel Orientation="Horizontal">
					<Label
						FontFamily="FontAwesome"
						Content="&#xF040;"
						Width="25"
						Height="32"
						FontSize="16"
						Padding="3,8"/>
					<Label
						Content="Вернуться к макету"
						Height="32"
						Width="175"
						FontSize="16"
						Padding="0,6"/>
				</StackPanel>
				
				</Button>
			
			<Border
				Grid.Column="0"
				Grid.Row="1"
				Height="20"
				CornerRadius="0,5,0,0"
				Background="White"
				/>

			<Border
				Grid.Column="2"
				Grid.Row="1"
				Height="20"
				CornerRadius="5,0,0,0"
				Background="White"
				/>

			<Border
				Grid.Column="1"
				Grid.Row="1"
				Height="20"
				CornerRadius="0,0,0,0"
				Background="White"
				/>

			<Border
				Grid.Column="1"
				Grid.Row="1"
				Height="20"
				CornerRadius="0,0,5,5"
				Background="LightGray"
				/>
			
		</Grid>
		
		
		<Grid
		   Name="badgeArea"
		   Margin="0,0"
		   ZIndex="1">

			<Grid.ColumnDefinitions>
				<ColumnDefinition ></ColumnDefinition>
				<ColumnDefinition Width="250" ></ColumnDefinition>
			</Grid.ColumnDefinitions>
			
			<StackPanel
				Name="itemEditionBlock"
				Grid.Column="0"
				Orientation="Horizontal">

				<Border
					Name="editionButtonsBlock"
					Width="130"
					Height="270"
					Margin="0,20"
					CornerRadius="0,5,5,0"
					Background="LightGray"
					ZIndex="2">
					
					<StackPanel>

						<StackPanel
							Name="ammounts"
							Orientation="Horizontal">

							<Label
								Name="entireCount"
								HorizontalAlignment="Left"
								HorizontalContentAlignment="Center"
								Width="65"
								FontSize="16"
								Content="{Binding  Path = ProcessableCount,  Mode=OneWay}"/>

							<Label
								Name="incorrectsCount"
								HorizontalAlignment="Left"
								HorizontalContentAlignment="Center"
								Width="65"
								FontSize="16"
								Foreground="Red"
								Content="{Binding  Path = IncorrectBadgesCount,  Mode=OneWay}"/>
						</StackPanel>

						<StackPanel
							Name="textSize"
							Orientation="Horizontal">

							<Button
								Name="zoomOut"
								Content="&#xF068;"
								ToolTip.Tip="Уменьшить размер шрифта на выделенной строке"
								HorizontalContentAlignment="Center"
								VerticalContentAlignment="Center"
								Width="32"
								Height="32"
								FontSize="16"
								FontFamily="FontAwesome"
								Margin="5,0"
								KeyDown="ToSide"
								Command="{Binding  Path = ReduceFontSize}"
								IsEnabled="{Binding   Path = ZoommerIsEnable,  Mode=OneWay}" />

							<Label
								Name="scalabilityGrade"
								Width="50"
								Height="32"
								FontSize="16"
								Margin="-2,0"
								GotFocus="HandleGettingFocus"
								KeyDown="ToParticularBadge"
								Content="{Binding  Path = BeingProcessedBadge.FocusedFontSize,  Mode=OneWay}"
								HorizontalContentAlignment="Center"
								VerticalContentAlignment="Center"
								BorderBrush="black"
								CornerRadius = "3"
								BorderThickness="1"
								IsTabStop="False"
								Focusable="False"
								IsEnabled="{Binding  Path = ZoommerIsEnable,  Mode=OneWay}"/>

							<Button
								Name="zoomOn"
								Content="&#xF067;"
								ToolTip.Tip="Увеличить размер шрифта на выделенной строке"
								HorizontalContentAlignment="Center"
								VerticalContentAlignment="Center"
								Width="32"
								Height="32"
								FontSize="16"
								FontFamily="FontAwesome"
								Margin="5,0"
								KeyDown="ToSide"
								Command="{Binding  Path = IncreaseFontSize}"
								IsEnabled="{Binding   Path = ZoommerIsEnable,  Mode=OneWay}" />
						</StackPanel>

						<TextBox
							Name="editorTextBox"
							Width="120"
							Height="120"
							FontSize="16"
							Margin="0,7"
							HorizontalAlignment="Center"
							TextWrapping="Wrap"
							IsEnabled="{Binding  Path = ZoommerIsEnable,  Mode=OneWay}"
							Text="{Binding  Path = BeingProcessedBadge.FocusedText,  Mode=OneWay}"
							KeyDown="ToParticularBadge"
							GotFocus="HandleGettingFocus"
							TextChanged="HandleTextEdition"/>

						<Button
							Name="spliter"
							ToolTip.Tip="Разделить выделенную строку на слова"
							HorizontalAlignment="Center"
							Width="120"
							Height="32"
							FontSize="16"
							Margin="0,1"
							Command="{Binding   Path = Split}"
							KeyDown="ToParticularBadge"
							IsEnabled="{Binding   Path = SplitterIsEnable,  Mode=OneWay}">

							<StackPanel Orientation="Horizontal">
								<Label
									FontFamily="FontAwesome"
									Content="&#xF040;"
									Width="20"
									Height="32"
									FontSize="16"
									Padding="0,6"/>

								<Label
									Content="Разделить"
									Height="32"
									Width="100"
									FontSize="16"
									Padding="0,6"/>
							</StackPanel>
							
						</Button>

						<Button
							Name="cancel"
							ToolTip.Tip="Отменить изменения на данном бэйдже"
							Width="120"
							Height="32"
							FontSize="16"
							HorizontalAlignment="Center"
							Margin="0,5"
							IsEnabled="{Binding  Path = BeingProcessedBadge.IsChanged,  Mode=OneWay }"
							Command="{Binding  Path = CancelChanges}"
							KeyDown="ToParticularBadge">

							<StackPanel Orientation="Horizontal">
								<Label
									FontFamily="FontAwesome"
									Content="&#xF0E2;"
									Width="20"
									Height="32"
									FontSize="16"
									Padding="0,6"/>

								<Label
									Content="Отмена"
									Height="32"
									Width="100"
									FontSize="16"
									Padding="10,6"/>
							</StackPanel>
							
						</Button>
					
					</StackPanel>
				</Border>
				
				<Border
					Name="workArea"
					HorizontalAlignment="Left"
					Width="{Binding  Path=WorkAreaWidth,  Mode=OneWay}"
					Height="380"
					Margin="-130, 0"
					Background="AliceBlue"
					BorderThickness="0">

					<ScrollViewer
						Name="scroller"
						VerticalScrollBarVisibility="Auto"
						HorizontalScrollBarVisibility="Auto"
						AllowAutoHide="True">

						<Border
							Name="badge"
							BorderThickness="1"
							BorderBrush="Gray"
							Width="{Binding  Path = BeingProcessedBadge.BorderWidth,  Mode=OneWay }"
							Height="{Binding  Path = BeingProcessedBadge.BorderHeight,  Mode=OneWay }"
							VerticalAlignment="Center"
							HorizontalAlignment="Center">

							<Canvas
								Name="incorrectBadge"
								VerticalAlignment="Center"
								HorizontalAlignment="Center"
								Width="{Binding  Path = BeingProcessedBadge.BadgeWidth,  Mode=OneWay }"
								Height="{Binding  Path = BeingProcessedBadge.BadgeHeight,  Mode=OneWay }">

								<Image
									Name="backgroundImage"
									Canvas.Top="0"
									Canvas.Left="0"
									Width="{Binding  Path = BeingProcessedBadge.BadgeWidth,  Mode=OneWay }"
									Height="{Binding  Path = BeingProcessedBadge.BadgeHeight,  Mode=OneWay }"
									VerticalAlignment="Top"
									KeyDown="ToSide"
									Source = "{Binding  Path = BeingProcessedBadge.ImageBitmap,  Mode=OneWay }"/>

								<ItemsControl
									Name="textLines"
									ItemsSource="{Binding  Path = BeingProcessedBadge.TextLines,  Mode=OneWay }">

									<ItemsControl.ItemsPanel>
										<ItemsPanelTemplate>
											<Canvas
												Width="{Binding  $parent[Canvas].Width,  Mode=OneWay }"
												Height="{Binding  $parent[Canvas].Height,  Mode=OneWay }"/>
										</ItemsPanelTemplate>
									</ItemsControl.ItemsPanel>

									<ItemsControl.Styles>
										<Style Selector="ItemsControl ContentPresenter">
											<Setter Property="Canvas.Left" Value="{Binding Path = LeftOffset,  Mode=TwoWay}" />
											<Setter Property="Canvas.Top" Value="{Binding Path = TopOffset,  Mode=TwoWay}" />
										</Style>
									</ItemsControl.Styles>

									<ItemsControl.ItemTemplate>
										<DataTemplate>

											<Border
												Background="{Binding   Path = Background,  Mode=OneWay }"
												BorderThickness="1"
												Height="{Binding Path = FontSize,  Mode=OneWay}">

												<TextBlock
													VerticalAlignment="Bottom"
													TextAlignment="Center"
													Height="{Binding  Path = FontSize,  Mode=OneWay}"
													FontSize="{Binding  Path = FontSize,  Mode=OneWay}"
													FontFamily="{Binding   Path = FontFamily,  Mode=OneWay }"
													FontWeight="{Binding   Path = FontWeight,  Mode=OneWay }"
													Foreground="{Binding   Path = Foreground,  Mode=OneWay }"
													DoubleTapped="Focus"
													PointerPressed="Capture"
													PointerMoved="Move"
													PointerEntered="SetCrossCursor"
													PointerExited="SetArrowCursor"
													KeyDown="ToSide"
													IsEnabled="True"
													Text="{Binding   Path = Content,  Mode=OneWay }" />

											</Border>

										</DataTemplate>
									</ItemsControl.ItemTemplate>
								</ItemsControl>

							</Canvas>
						</Border>

					</ScrollViewer>
				</Border>

			</StackPanel>

			
			<Border
				Name="collectionManagement"
				Grid.Column="1"
				Width="248"
				Margin="{Binding  Path=CollectionFilterMargin,  Mode=OneWay}"
				Height="380"
				CornerRadius="5,0,0,5"
				PointerWheelChanged="ScrollByWheel"
				Background="LightGray">
						
				<StackPanel
					Name="sliderPanel"
					Width="248"
					Height="380">

					<Button
						Name="extender"
						Classes="transparent"
						CornerRadius="5,0,0,0"
						HorizontalAlignment="Left"
						HorizontalContentAlignment="Left"
						VerticalContentAlignment="Center"
						Width="20"
						Height="30"
						Padding="4,5"
						FontFamily="FontAwesome"
						Command="{Binding  Path=ExtendOrShrinkCollectionManagement}"
						ToolTip.Tip="{Binding  Path=ExtentionTip,  Mode=OneWay}"
						Content="{Binding  Path=ExtenderContent,  Mode=OneWay}" />
					
					<Border
						Name="filterSwitcher"
						Height="24"
						Margin="7,5"
						KeyDown="ToParticularBadge">

						<StackPanel
							Orientation="Horizontal">

							<Button
								Name="switcher"
								Width="36"
								Height="22"
								ToolTip.Tip="Переключить фильтр"
								Command="{Binding  Path = Filter}"
								KeyDown="ToParticularBadge">

								<Button.Background>
									<ImageBrush
										Source = "{Binding  Path = FilterState,  Mode=OneWay }"/>
								</Button.Background>
							</Button>


						</StackPanel>
					</Border>

					<Button
						Name="up"
						Classes="transparent"
						Width="{Binding   Path = UpDownWidth,   Mode=OneWay}"
						Height="20"
						Padding="0,0"
						Margin ="15,5"
						FontSize="14"
						Focusable="{Binding   Path = UpDownIsFocusable,   Mode=OneWay}"
						FontFamily="FontAwesome"
						Command="{Binding   Path = ToPrevious}"
						IsEnabled="{Binding   Path = PreviousIsEnable,   Mode=OneWay}"
						Content="&#xF077;"/>
					
					<Border
						Name="slider"
						Width="240"
						Height="{Binding  Path=ScrollHeight,  Mode=OneWay}"
						Margin ="7,0"
						CornerRadius="0,0,0,5"
						KeyDown="ToParticularBadge">
						
					<StackPanel
						Orientation="Horizontal">

						<ItemsControl
							Name="badgeStates"
							Width="220"
							HorizontalAlignment="Left"
							ItemsSource="{Binding  Path=VisibleIcons,  Mode=OneWay}">

							<ItemsControl.ItemTemplate>
								<DataTemplate>
										
									<Grid Width="220"
										  Margin="0,2"
										  Tapped="ToParticularBadge">
										
										<Grid.ColumnDefinitions>
											<ColumnDefinition  Width="38"></ColumnDefinition>
											<ColumnDefinition></ColumnDefinition>
										</Grid.ColumnDefinitions>
										
										<Border
											Grid.Column="0"
											HorizontalAlignment="Left"
											Width="36"
											Height="24"
											BorderBrush="{Binding  Path = BorderColor,  Mode=OneWay }"
											BorderThickness="1">

											<Image
												Source = "{Binding  Path = CorrectnessIcon,  Mode=OneWay }"
												Width="36"
												Height="22"
												HorizontalAlignment="Left"
												Opacity="{Binding  Path = IconOpacity,  Mode=OneWay }"
												/>

										</Border>

										<Label 
											Grid.Column="1"
											Padding="5,3"
											FontWeight="{Binding  Path = BoundFontWeight,  Mode=OneWay }"
											Content="{Binding  Path = BoundPersonName,  Mode=OneWay }"
											/>
											
											
									</Grid>
								</DataTemplate>
							</ItemsControl.ItemTemplate>
						</ItemsControl>

						<Grid
							Name="scroll"
							Width="{Binding   Path = ScrollWidth,  Mode=OneWay}"
							Height="{Binding   Path = ScrollHeight,  Mode=OneWay}"
							Background="White">
							
							<Grid.RowDefinitions>
								<RowDefinition Height="15"></RowDefinition>
								<RowDefinition Height="{Binding   Path = RunnerBruttoWalkSpace,  Mode=OneWay}"></RowDefinition>
								<RowDefinition Height="15"></RowDefinition>
							</Grid.RowDefinitions>
							

							<Button
								Name="upper"
								Classes="transparent"
								Grid.Row="0"
								Width="{Binding   Path = ScrollWidth,  Mode=OneWay}"
								Height="15"
								Padding="2,1"
								FontFamily="FontAwesome"
								Content="&#xF077;"
								FontSize="9"
								Focusable="{Binding   Path = UpDownIsFocusable,   Mode=OneWay}"
								Command="{Binding Path=ScrollUp}"
								PointerPressed=""/>

							<Canvas
								Name="runnerWay"
								Grid.Row="1"
								Background="White"
								Width="{Binding   Path = ScrollWidth,  Mode=OneWay}"
								Height="{Binding   Path = RunnerBruttoWalkSpace,  Mode=OneWay}"
								VerticalAlignment="Center"
								Tapped="ShiftRunner">
							
								<Canvas
									Name="runner"
									Width="{Binding   Path = ScrollWidth,  Mode=OneWay}"
									Height="{Binding   Path = RunnerHeight,  Mode=OneWay}"
									Canvas.Left="0"
									Canvas.Top="{Binding   Path = ScrollOffset,  Mode=OneWay}"
									Background="Red"/>
								
								</Canvas>

							<Button
								Name="downer"
								Classes="transparent"
								Grid.Row="2"
								Width="{Binding   Path = ScrollWidth,  Mode=OneWay}"
								Height="15"
								Padding="2,1"
								FontSize="9"
								FontFamily="FontAwesome"
								Focusable="{Binding   Path = UpDownIsFocusable,   Mode=OneWay}"
								Content="&#xF078;"
								Command="{Binding Path=ScrollDown}"/>

						</Grid>
						
					</StackPanel>	
						
					</Border>
				
				    <Button
						Name="down"
						Classes="transparent"
						Width="{Binding   Path = UpDownWidth,   Mode=OneWay}"
						Height="20"
						Padding="0,0"
						Margin ="15,5"
						FontSize="14"
						Focusable="{Binding   Path = UpDownIsFocusable,   Mode=OneWay}"
						FontFamily="FontAwesome"
						Command="{Binding   Path = ToNext}"
						IsEnabled="{Binding   Path = NextIsEnable,   Mode=OneWay}"
						Content="&#xF078;"/>
				
				</StackPanel>
				
			</Border>
		
		</Grid>


		<Grid
			Name="navigation"
			Height="40"
			Background="LightGray"
			Margin="0,0">

			<Grid.ColumnDefinitions>
				<ColumnDefinition ></ColumnDefinition>
				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition Width="64"></ColumnDefinition>
				
				<ColumnDefinition Width="50"></ColumnDefinition>
				<ColumnDefinition Width="50"></ColumnDefinition>
				
				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition ></ColumnDefinition>


				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition Width="64"></ColumnDefinition>
				<ColumnDefinition Width="34"></ColumnDefinition>
				<ColumnDefinition Width="2"></ColumnDefinition>
				
			</Grid.ColumnDefinitions>


			<Button
				Name="firstBadge"
				Classes="transparent"
				ToolTip.Tip="На первый бэйдж"
				Grid.Column="1"
				HorizontalContentAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="7,7"
				Command="{Binding   Path = ToFirst}"
				FontFamily="FontAwesome"
				Content="&#xF049;"
				IsEnabled="{Binding   Path = FirstIsEnable,   Mode=OneWay}" />

			<Button
				Name="previousBadge"
				Classes="transparent"
				ToolTip.Tip="На предыдущий бэйдж"
				Grid.Column="2"
				HorizontalContentAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="5,7"
				Command="{Binding   Path = ToPrevious}"
				FontFamily="FontAwesome"
				Content="&#xF04A;"
				IsEnabled="{Binding   Path = PreviousIsEnable,   Mode=OneWay}"/>

			<TextBox
				Name="visibleBadgeNumber"
				Classes="transparent"
				Width="60"
				Grid.Column="3"
				Height="32"
				FontSize="16"
				Margin="0,3"
				Focusable="False"
				IsReadOnly="True"
				Text="{Binding   Path = BeingProcessedNumber,   Mode=OneWay}"/>
			
			<Label
				Content="Из"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				FontSize="16"
				Height="32"
				Grid.Column="4"/>

			<Label
				Content="{Binding  Path = ProcessableCount,  Mode=OneWay}"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				FontSize="16"
				Height="32"
				Grid.Column="5"/>

			<Button
				Name="nextBadge"
				Classes="transparent"
				ToolTip.Tip="На следующий бэйдж"
				Grid.Column="6"
				HorizontalContentAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="8,7"
				Command="{Binding   Path = ToNext}"
				FontFamily="FontAwesome"
				Content="&#xF04E;"
				IsEnabled="{Binding   Path = NextIsEnable,   Mode=OneWay}" />

			<Button
				Name="lastBadge"
				Classes="transparent"
				ToolTip.Tip="На последний бэйдж"
				Grid.Column="7"
				HorizontalContentAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="6,7"
				Command="{Binding   Path = ToLast}"
				FontFamily="FontAwesome"
				Content="&#xF050;"
				IsEnabled="{Binding   Path = LastIsEnable,   Mode=OneWay}"/>

			<Button
				Name="zoomOutBadge"
				Classes="transparent"
				Grid.Column="9"
				HorizontalAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="8,7"
				IsEnabled="{Binding   Path = ZoomOutIsEnable,   Mode=OneWay}"
				Command="{Binding   Path = ZoomOut}"
				ToolTip.Tip="Уменьшить страницу"
				FontFamily="FontAwesome"
				Content="&#xF068;"/>

			<Label
				Name="badgeScalabilityGrade"
				Grid.Column="10"
				Width="60"
				Height="32"
				FontSize="16"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				BorderBrush="black"
				CornerRadius = "3"
				BorderThickness="1"
				Content="{Binding  Path = ZoomDegreeInView,  Mode=OneWay}"
				/>

			<Button
				Name="zoomOnBadge"
				Classes="transparent"
				Grid.Column="11"
				Margin="0,3"
				HorizontalAlignment="Center"
				Width="32"
				Height="32"
				FontSize="16"
				Padding="9,7"
				IsEnabled="{Binding   Path = ZoomOnIsEnable,   Mode=OneWay}"
				Command="{Binding   Path = ZoomOn}"
				ToolTip.Tip="Увеличить страницу"
				FontFamily="FontAwesome"
				Content="&#xF067;" />


		</Grid>

	</StackPanel>
</UserControl>


